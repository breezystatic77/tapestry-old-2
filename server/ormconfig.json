{
	"type": "sqlite",
	"database": ":memory:",
	"dropSchema": true,
	"synchronize": true,
	"logging": false,
	"entities": ["src/db/entity/**/!(*.spec).ts"],
	"migrations": ["src/db/migration/**/!(*.spec).ts"],
